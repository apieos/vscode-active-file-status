{
    "name": "vscode-active-file-status",
    "displayName": "Active File Status",
    "description": "Displays the active file in the status bar.",
    "version": "1.0.0",
    "publisher": "sgtsquiggs",
    "repository": {
        "type": "git",
        "url": "https://github.com/sgtsquiggs/vscode-active-file-status"
    },
